#wCircularStorage, #wCircularTransfer
{
qproperty-outerCircleBackgroundColor: #f3f4f4; /*colorToken.surface-2*/
qproperty-innerCircleBackgroundColor: #ffffff; /*colorToken.page-background*/

qproperty-lightOkProgressBarColors: #477ef7; /*colorToken.indicator-indigo*/
qproperty-darkOkProgressBarColors: #477ef7; /*colorToken.indicator-indigo*/
qproperty-lightWarnProgressBarColors: #fb6514; /*colorToken.indicator-orange*/
qproperty-darkWarnProgressBarColors: #fb6514; /*colorToken.indicator-orange*/
qproperty-lightFullProgressBarColors: #F63D6B; /*colorToken.indicator-pink*/
qproperty-darkFullProgressBarColors: #F63D6B; /*colorToken.indicator-pink*/

qproperty-okStateTextColor: #303233; /*colorToken.text-primary*/
}

#pLoading, #pStorageAndTransferUsage, #wStorageUsage, #wTransferUsage, #wStorageDetails, #wTransferDetails
{
background-color: transparent;
}

QGroupBox
{
margin-top: 20px;
margin-right: 20px;
margin-left: 20px;
margin-bottom: 20px;
}

#line
{
background-color: #f3f4f4; /*colorToken.surface-2*/
border-style: none;
}

#headerBox
{
border-style:none;
border-radius:20px;
background-color: #fafafa; /*colorToken.surface-1*/
}

QProgressBar
{
height: 4px;
min-height: 4px;
max-height: 4px;
border:0px;
border-radius: 2px;
background-color: #f3f4f4; /*colorToken.surface-2*/
margin-bottom: 4px;
}

QProgressBar::chunk
{
background-color: #477EF7; /*colorToken.indicator-indigo*/
border-radius: 2px;
border:0px;
}

#wDetailedUsage QPushButton, #bVersionIcon
{
border-style: none;
background-color: transparent;
padding-left: 10px;
padding-right: 10px;
height: 22px;
color: transparent;
font-weight: bold;
}

#AccountDetailsDialog
{
border-radius: 0 0 4px 4px;
}

QLabel
{
font-family: &quot;Lato&quot;;
background-color: transparent;
color: #303233; /*colorToken.text-primary*/
font-size: 13px;
}

#sHeader
{
    background-color: transparent;
    height: 96px;
    width: 440px;
    height: 96px;
    width: 440px;
    max-height: 96px;
    min-height: 96px;
    max-width:440px;
    min-width:440px;
}

#lCloudDrive, #lVault, #lRubbish, #lStorage, #lTransfer, #lAvailable
{
  font-weight: bold;
}

#lStorage, #lTransfer
{
  qproperty-alignment: 'AlignLeft | AlignVCenter';
}

#lCloudDrive, #lVault, #lRubbish, #lAvailable
{
    qproperty-alignment: 'AlignLeft | AlignBottom';
}

#wCloudDrive, #wRubbish, #wVault
{
    border: 0px;
}

#lStorage, #lTransfer, #lUsedStorage, #lUsedTransfer
{
    margin-left: 6px;
}

#lStorage, #lTransfer
{
    padding-bottom: 4px;
}

#lUsedCloudDrive, #lUsedVault,  #lUsedRubbish
{
    qproperty-alignment: 'AlignRight | AlignBottom';
    padding-right: 4px;
}

#lUsedByVersions
{
    qproperty-alignment: 'AlignRight | AlignVCenter';
    padding-right: 8px;
}

#wDetailedUsage
{
    min-height: 120px;
}

#AccountDetailsDialog[storageState=&quot;warning&quot;] #wDetailedUsage QProgressBar::chunk
{
  background-color: #FF993B;
}

#AccountDetailsDialog[storageState=&quot;full&quot;] #wDetailedUsage  QProgressBar::chunk
{
  background-color: #FF5646;
}

#bCloudDrive, #bVault, #bRubbish
{
    qproperty-iconSize: 24px;
}

#lLoading
{
    border-radius: 16px;
    background-color: transparent;
    height: 32px;
    max-height: 32px;
    min-height: 32px;
    max-width:440px;
    min-width:10px;
    font-size: 14px;
    color: #303233; /*colorToken.text-primary*/
    padding-left:24px;
    padding-right:24px;
    qproperty-alignment: 'AlignVCenter | AlignHCenter';
}

#pLoading, #pStorageAndTransferUsage
{
    height: 96px;
    width: 440px;
    max-height: 96px;
    min-height: 96px;
    max-width:440px;
    min-width:440px;
}

#AccountDetailsDialog[loading=true]  #sHeader
{
    /* Loading page */
    qproperty-currentIndex: 0;
}

#AccountDetailsDialog[loading=false]  #sHeader
{
    /* Usage page */
    qproperty-currentIndex: 1;
}

#AccountDetailsDialog[accountType=&quot;free&quot;] #pStorageAndTransferUsage QStackedWidget, #AccountDetailsDialog[accountType=&quot;pro&quot;] #pStorageAndTransferUsage QStackedWidget
{
    /* Business account pages */
    qproperty-currentIndex: 0;
}

#AccountDetailsDialog[accountType=&quot;business&quot;]  #pStorageAndTransferUsage QStackedWidget
{
    /* Non-business account pages */
    qproperty-currentIndex: 1;
}

#pStorageAndTransferUsage #sStorage, #sTransfer
{
height: 65px;
width: 70px;
min-height: 65px;
min-width: 70px;
max-height: 65px;
max-width: 70px;
margin-left: 20px;
background-color: transparent;
}

#wStorageUsage, #wTransferUsage
{
height: 96px;
width: 219px;
max-height: 96px;
min-height: 96px;
max-width:219px;
min-width:219px;
}

#wStorageUsage
{
    border-right: 1px solid transparent;
}

#wTransferUsage
{
    border-left: 1px solid transparent;
}

#pStorageAndTransferUsage QPushButton
{
    qproperty-iconSize: 44px;
    border:0px;
}

#AccountDetailsDialog[accountType=&quot;business&quot;] #bBusinessStorage
{
    qproperty-icon :url(:/images/storage_for_business.png);
}

#AccountDetailsDialog[accountType=&quot;business&quot;]  #bBusinessTransfer
{
    qproperty-icon :url(:/images/transfer_for_business.png);
}
